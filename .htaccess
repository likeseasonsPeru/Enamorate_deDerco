RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^(.*)$ $1.html


# RewriteRule inicio.php
RewriteRule ^inicio?$ inicio.php

# RewriteRule fltro.php
RewriteRule ^filtro?$ filtro.php

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# RewriteRule filtr.php
RewriteRule ^([A-Za-z0-9]+)?$ models/cotizador.php [L,QSA]

# RewriteRule cotizador.php
RewriteRule ^([A-Za-z0-9-]+)/([A-Za-z0-9]+)/?$ models/cotizador.php?modelo=$1&marca=$2 [L,QSA]



